<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SampleAppKelasB.HomePage" Title="Home Page">
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="menuProductList" Text="Product List" Order="Secondary" />
        <ToolbarItem x:Name="menuSimpleList" Text="Simple List" Order="Secondary" />
        <ToolbarItem x:Name="menuTextList" Text="Text List" Order="Secondary" />
        <ToolbarItem x:Name="menuKalkulator" Icon="menucalculator.png" Order="Primary" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="btnSimpleList" Text="Simple List View" Clicked="btnSimpleList_Clicked" />
            <Button x:Name="btnTextListView" Text="Text List View" Clicked="btnTextListView_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>